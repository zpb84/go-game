{
  // Используйте IntelliSense, чтобы узнать о возможных атрибутах.
  // Наведите указатель мыши, чтобы просмотреть описания существующих атрибутов.
  // Для получения дополнительной информации посетите: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/console"
    },
    {
      "name": "Run bots",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/console",
      "args": ["-bots"]
    },
    {
      "name": "Attach",
      "type": "go",
      "request": "launch",
      "mode": "remote",
      "remotePath": "${workspaceFolder}/cmd/console",
      "port": 2345,
      "host": "127.0.0.1",
      "program": "${workspaceFolder}/cmd/console/__debug_bin",
      "env": {},
      "args": []
    }
  ]
}